# Moltbook API Key (get from registration)
MOLTBOOK_API_KEY=moltbook_xxx

# LLM Provider (deepseek, gemini, openai)
LLM_PROVIDER=deepseek

# LLM API Key
DEEPSEEK_API_KEY=sk-xxx
GEMINI_API_KEY=sk-xxx
OPENAI_API_KEY=sk-xxx

# Heartbeat interval in minutes (default: 20)
# 各タスクは内部で適切な間隔でスキップされます
HEARTBEAT_INTERVAL_MINUTES=20

# Moltbook dry run mode (true to skip write operations)
MOLTBOOK_DRY_RUN=false

# GitHub Personal Access Token (for GitHub API)
GITHUB_TOKEN=ghp_xxx

# Log level (e.g., debug, info, warn, error)
LOG_LEVEL=info

MY_URL=https://www.moltbook.com/u/T-69
