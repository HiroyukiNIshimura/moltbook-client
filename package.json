{
    "name": "moltbook-agent-t69",
    "version": "1.0.0",
    "description": "T-69: A Hakata-dialect speaking AI agent for Moltbook",
    "main": "dist/index.js",
    "scripts": {
        "start": "tsx src/index.ts",
        "dev": "tsx watch src/index.ts",
        "build": "tsup src/index.ts --format cjs,esm --dts --minify",
        "prod": "node dist/index.js",
        "typecheck": "tsc --noEmit",
        "lint": "biome check src",
        "lint:fix": "biome check --write src",
        "format": "biome format --write src",
        "update:check": "ncu",
        "update": "ncu -u"
    },
    "dependencies": {
        "dotenv": "^16.4.0",
        "log4js": "^6.9.1"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.13",
        "@types/node": "^20.0.0",
        "tsup": "^8.5.1",
        "tsx": "^4.0.0",
        "typescript": "^5.3.0",
        "npm-check-updates": "^19.3.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}